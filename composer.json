{
  "name": "mts7/php-execution-timer",
  "description": "Testable PHP timer",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "mts7/php-code-quality": "^0.1.18",
    "phpunit/phpunit": "^9.6",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "MtsTimer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MtsTimer\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
