{
  "name": "mts7/php-base-template",
  "description": "PHP Base Template",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "infection/infection": "^0.26.16",
    "phing/phing": "^2.17",
    "phpmd/phpmd": "^2.13",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "roave/psalm-html-output": "^1.0",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.6",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.29"
  },
  "autoload": {
    "psr-4": {
      "MtsProject\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MtsProject\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
